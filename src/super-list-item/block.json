{
    "$schema": "https://schemas.wp.org/trunk/block.json",
    "apiVersion": 3,
    "name": "enhanced-super-list/super-list-item",
    "version": "1.0.0",
    "title": "Enhanced Super List Item",
    "category": "text",
    "icon": "editor-ul",
    "description": "A list item that can contain multiple nested blocks.",
    "keywords": ["list", "item", "nested", "blocks"],
    "textdomain": "enhanced-super-list",
    "parent": ["enhanced-super-list/super-list"],
    "attributes": {
        "content": {
            "type": "string",
            "source": "html",
            "selector": ".wp-block-enhanced-super-list-super-list-item__content"
        },
        "customMarker": {
            "type": "string",
            "default": ""
        },
        "markerIcon": {
            "type": "string",
            "default": ""
        },
        "markerColor": {
            "type": "string",
            "default": ""
        },
        "completed": {
            "type": "boolean",
            "default": false
        }
    },
    "supports": {
        "html": false,
        "inserter": false,
        "reusable": false,
        "color": {
            "gradients": true,
            "background": true,
            "text": true
        },
        "spacing": {
            "margin": true,
            "padding": true
        },
        "typography": {
            "fontSize": true,
            "lineHeight": true,
            "__experimentalFontFamily": true,
            "__experimentalFontWeight": true,
            "__experimentalFontStyle": true,
            "__experimentalTextTransform": true,
            "__experimentalTextDecoration": true,
            "__experimentalLetterSpacing": true
        },
        "__experimentalBorder": {
            "color": true,
            "radius": true,
            "style": true,
            "width": true
        }
    },
    "usesContext": [
        "enhanced-super-list/ordered",
        "enhanced-super-list/type",
        "enhanced-super-list/orientation"
    ],
    "selectors": {
        "root": ".wp-block-enhanced-super-list-super-list-item"
    },
    "editorScript": "file:./index.js",
    "editorStyle": "file:./index.css",
    "style": "file:./style-index.css"
}